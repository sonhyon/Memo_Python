import pandas as pd

# 예제 데이터 생성
data = {
    'name': ['Alice', 'Bob', 'Charlie', 'Diana', 'Ethan'],
    'age': [25, 30, 22, 28, 35],
    'gender': ['Female', 'Male', 'Male', 'Female', 'Male'],
    'score': [85, 78, 92, 88, 70]
}

df = pd.DataFrame(data)
print(df)

#Q. ‘score’ 열이 80 이상인 데이터의 개수를 구하세요.
df[df['score'] >= 80].shape[0]
