import pandas as pd

# 예제 데이터 생성
data = {
    'name': ['Alice', 'Bob', 'Charlie', 'Diana', 'Ethan'],
    'age': [25, 30, 22, 28, 35],
    'gender': ['Female', 'Male', 'Male', 'Female', 'Male'],
    'score': [85, 78, 92, 88, 70]
}

df = pd.DataFrame(data)
print(df)

#Q. CSV 파일에서 ‘age’ 열의 평균을 구하세요.
average_age = df['age'].mean()
print(average_age)
